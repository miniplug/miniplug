{
  "name": "miniplug",
  "description": "Teeny-tiny plug.dj library/API",
  "version": "0.0.0",
  "license": "MIT",
  "keywords": [
    "plug",
    "plug.dj"
  ],
  "repository": "goto-bus-stop/miniplug",
  "bugs": "https://github.com/goto-bus-stop/miniplug/issues",
  "author": "Ren√© Kooi <rene@kooi.me>",
  "dependencies": {
    "bluebird": "^3.4.6",
    "debug": "^2.3.3",
    "flatten": "^1.0.2",
    "lodash.partial": "^4.2.1",
    "object-assign": "^4.1.0",
    "plug-login": "^0.4.0",
    "plug-socket": "^0.1.0",
    "request": "^2.60.0",
    "unescape": "^0.1.1"
  },
  "devDependencies": {
    "buble": "^0.14.2",
    "mocha": "3.2.0",
    "rollup": "^0.36.4",
    "rollup-plugin-commonjs": "^5.0.5",
    "rollup-plugin-node-resolve": "^2.0.0",
    "standard": "^8.6.0"
  },
  "scripts": {
    "prepublish": "npm run build",
    "build:buble": "buble src --output es --no modules --objectAssign Object.assign",
    "build:rollup": "rollup -c rollup.config.js",
    "build": "npm run build:buble && npm run build:rollup",
    "lint": "standard gulpfile.js src/**/*.js test/**/*.js",
    "test": "npm run lint && mocha --require buble/register"
  },
  "standard": {
    "env": "mocha"
  }
}
